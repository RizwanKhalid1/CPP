#include<iostream>
#include<conio.h>
#include<string>
using namespace std;
int main()
{
	string name[5];
	for (int i = 0; i < 5; i++)
	{
		getline(cin, name[i]);
	}
	cout << "The name of countries that start with vowel:";
	for (int i = 0; i < 5; i++)
	{
		switch (name[i][0])
		{
		case 'A':
		case'E':
		case'I':
		case'O':
		case'U':
			cout << name[i] << endl;
		}
		
	}


	_getch();
	return 0;
}
