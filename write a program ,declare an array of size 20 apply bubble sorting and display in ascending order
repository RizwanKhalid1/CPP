Code :
#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
	int a[20]={20,12,56,32,78,12,89,65,34,29,67,16,70,80,90,40,30,21,13,44};
	int temp;
	cout<<"The number of 20 arrays:";
	for(int i=0;i<20;i++)
	{
		cout<<a[i]<<endl;
	}
		for(int i=0;i<=20;i++)
		{
			for(int j=0;j<20;j++)
			{
				if (a[j+1]<a[j])
				{
					temp=a[j];
					a[j]=a[j+1];
					a[j+1]=temp;
				}
			}
		}
	
		cout<<"The sorted number are ";
				for(int b=0;b<20;b++)
				{
					cout<<a[b]<<" ";
				}
	
				_getch();
				return 0;
}



