#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
	int a[]={12,76,87,54,23,98,56,42,91,32};
	int even,odd,i;
	cout<<"The elemnt of array:"<<endl;
	for(i=0;i<=9;i++)
	{
		cout<<a[i]<<endl;
	}
	even=0;
	odd=0;
	for(i=0;i<=9;i++)
	{
		if(a[i]%2==0)
		{
			cout<<even++;
		}
		else
		{
			cout<<odd++;
		}
	}
	cout<<"Total even elements:"<<even<<endl;
	cout<<"Total odd element :"<<odd;
	_getch();
	return 0;
}

