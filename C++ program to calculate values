#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
	int num1, num2, sum, subtract, product;
	cout << "Enter num1";
	cin >> num1;
	cout << "Enter num2";
	cin >> num2;
	sum = num1 + num2;
	cout << "Sum of num1 and num2 is:" << sum << endl;
	subtract = num1 - num2;
	cout << "Subtraction of num1 and num2 is:" << subtract << endl;
	product = num1*num2;
	cout << "Product of num1 and num2 is:" << product << endl;
	_getch();
	return 0;
}
