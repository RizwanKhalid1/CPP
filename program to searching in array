#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
	int a[20]={20,12,56,32,78,12,89,65,34,29,67,16,70,80,90,40,30,21,13,44};
	int key;
	cout<<"The number of 20 arrays:"<<endl;
	for(int i=0;i<20;i++)
	{
		cout<<a[i]<<endl;
	}
	cout<<"Enter a key to search"<<endl;
	cin>>key;
	cout<<endl;
	for(int i=0;i<20;i++)
	{
		if(key==a[i])
		{
			cout<<"Key found at index : "<<i<<endl;
			break;
		}
	}
				_getch();
				return 0;
}


