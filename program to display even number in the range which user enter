#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
	int s, e;
	cout << "Enter the starting number  ";
	cin >> s;
	cout << "Enter the Ending Number   ";
	cin >> e;
	cout << endl;
	for (int i = s; i <= e; i++)
	{
		if (i % 2 == 0)
		{
			cout << i << "  ";
		}
	}
	cout << endl << "These are the even numbers";
	_getch();
	return 0;
}
